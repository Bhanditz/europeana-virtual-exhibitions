<%- cache(element) do -%>
  <%= element_view_for(element) do |el| -%>
    <div class="background_image">
      <%= el.render :background_image %>
    </div>
    <div class="background_image_caption">
      <%= el.render :background_image_caption %>
    </div>
    <div class="title">
      <%= el.render :title %>
    </div>
    <div class="sub_title">
      <%= el.render :sub_title %>
    </div>
  <%- end -%>
<%- end -%>
