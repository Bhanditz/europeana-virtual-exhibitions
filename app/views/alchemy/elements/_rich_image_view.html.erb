<%- cache(element) do -%>
  <%= element_view_for(element) do |el| -%>
    <div class="title">
      <%= el.render :title %>
    </div>
    <div class="sub_title">
      <%= el.render :sub_title %>
    </div>
    <div class="image">
      <%= el.render :image %>
    </div>
    <div class="image_alignment">
      <%= el.render :image_alignment %>
    </div>
    <div class="image_credit">
      <%= el.render :image_credit %>
    </div>
    <div class="quote">
      <%= el.render :quote %>
    </div>
    <div class="quotee">
      <%= el.render :quotee %>
    </div>
    <div class="body">
      <%= el.render :body %>
    </div>
    <div class="hide_in_credits">
      <%= el.render :hide_in_credits %>
    </div>
  <%- end -%>
<%- end -%>
